{
  "name": "rxdb-example-graphql",
  "version": "1.1.0",
  "scripts": {
    "dev": "webpack-dev-server",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "node -r esm ./server/index.js",
    "server2": "node -r esm ./server/test.js",
    "refresh": "rimraf -r node_modules/rxdb/ && npm i ",
    "build": "webpack --env.ENV production",
    "build:serve": "ws -p 8888 -d dist/",
    "test:e2e": "testcafe chromium test/ --selector-timeout 30000",
    "test": "npm run build && concurrently \"npm run server\" \"npm run build:serve\" \"npm run test:e2e\" --kill-others --success first"
  },
  "author": "pubkey",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "2.8.1",
    "concurrently": "4.1.1",
    "cors": "2.8.5",
    "express-graphql": "0.9.0",
    "graphql": "14.4.2",
    "graphql-client": "2.0.1",
    "graphql-subscriptions": "1.1.0",
    "local-web-server": "3.0.7",
    "normalize.css": "8.0.1",
    "pouchdb-adapter-idb": "7.0.0",
    "rxdb": "../../",
    "subscriptions-transport-ws": "0.9.16"
  },
  "devDependencies": {
    "async-test-util": "1.7.3",
    "css-loader": "3.1.0",
    "eslint": "6.1.0",
    "esm": "3.2.25",
    "file-loader": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "ncp": "2.0.0",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "testcafe": "1.3.3",
    "testcafe-hammerhead": "14.7.1",
    "webpack": "4.38.0",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  }
}
